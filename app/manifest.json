{
  "background": {
    "scripts": [ "scripts/background.js" ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "render.html"
  },
  "default_locale": "en",
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "webRequest",
    "activeTab",
    "storage",
    "*://*/*"
  ],
  "version": "0.2"
}
